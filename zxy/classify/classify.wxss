/* pages/classify/classify.wxss */

#square{
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100px;
  border:solid black 1.3px;
  background: #dffadd;
}

#frame{
  margin: 100px 20px 100px 20px;
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
  grid-template-rows: 1fr 1fr 1fr;
  padding: 10px 10px 10px 10px;
  border: solid black 1.5px;
  /* width: 380px; */
  height: 400px; 
  justify-content: space-around;
  align-items: center;
  background-color: white;
}

#round{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100px;
  height: 100px;
  border-radius: 50%;
  border: solid rgb(63, 39, 39) 1.3px;
  background: #cee9d7;
}

#round1{
  width: 20px;
  height: 20px;
  border-radius: 50%;
  /* border: solid white; */
}

/* 背景 */
.bg{
  position:fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background:#25e264;
  background-size:cover;
  z-index:-1;
}

.bg-img{
  width : 100%;
  height : 100%;
}